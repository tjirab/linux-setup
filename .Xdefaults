URxvt.font: xft:Ubuntu Mono:size=10:antialias=true
URxvt.boldFont: xft:Ubuntu Mono:bold:size=10:antialias=true

! colours
URxvt*background: #000000
URxvt*foreground: #FFFFFF

! remove scrollbar
URxvt*scrollBar: false
URxvt*scrollBar_right: false

